{
  "name": "searchx-vscode",
  "description": "SearchX",
  "version": "0.0.1",
  "publisher": "zlh",
  "preview": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/vscode-extension-samples"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "icon": "media/favicon.png",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "node ./scripts/build.mjs",
    "watch": "node ./scripts/build.mjs --watch",
    "typecheck": "tsc --noEmit",
    "gen": "vsce package --no-dependencies"
  },
  "contributes": {
    "commands": [
      {
        "command": "searchx.searchInFolder",
        "title": "SG: Find searchx Pattern in Folder"
      },
      {
        "command": "searchx.clearSearchResults",
        "title": "Clear Search Results",
        "icon": {
          "light": "media/clear-light.svg",
          "dark": "media/clear-dark.svg"
        }
      },
      {
        "command": "searchx.refreshSearch",
        "title": "Refresh Search",
        "icon": {
          "light": "media/refresh-light.svg",
          "dark": "media/refresh-dark.svg"
        }
      },
      {
        "command": "searchx.collapseAll",
        "title": "Collapse All",
        "icon": {
          "light": "media/collapse-light.svg",
          "dark": "media/collapse-dark.svg"
        }
      },
      {
        "command": "searchx.expandAll",
        "title": "Expand All",
        "icon": {
          "light": "media/expand-light.svg",
          "dark": "media/expand-dark.svg"
        }
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "searchx.searchInFolder",
          "group": "4_search",
          "when": "explorerResourceIsFolder"
        }
      ],
      "view/title": [
        {
          "command": "searchx.collapseAll",
          "when": "view == searchx.search.input",
          "group": "navigation"
        },
        {
          "command": "searchx.expandAll",
          "when": "view == searchx.search.input",
          "group": "navigation"
        },
        {
          "command": "searchx.clearSearchResults",
          "when": "view == searchx.search.input",
          "group": "navigation"
        },
        {
          "command": "searchx.refreshSearch",
          "when": "view == searchx.search.input",
          "group": "navigation"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "searchx-sidebar-view",
          "title": "SearchX",
          "icon": "media/favicon.svg"
        }
      ]
    },
    "views": {
      "searchx-sidebar-view": [
        {
          "type": "webview",
          "id": "searchx.search.input",
          "name": "SearchX"
        }
      ]
    }
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/vscode": "^1.73.0",
    "@types/vscode-webview": "^1.57.0",
    "typescript": "^5.6.2",
    "@stylexjs/esbuild-plugin": "0.7.5",
    "@stylexjs/stylex": "0.7.5",
    "@types/react": "18.3.5",
    "@types/react-dom": "18.3.0",
    "@vscode/webview-ui-toolkit": "1.4.0",
    "esbuild": "0.23.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-icons": "5.3.0",
    "react-use": "17.5.1",
    "react-virtuoso": "4.10.1",
    "tslib": "2.7.0",
    "tsx": "4.19.0",
    "unport": "0.6.0",
    "dedent": "^0.7.0",
    "react-simple-code-editor": "^0.11.2"
  }
}