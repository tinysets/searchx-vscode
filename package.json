{
  "name": "searchx-vscode",
  "displayName": "SearchPro",
  "description": "Search by keywords.",
  "version": "1.0.0",
  "publisher": "Leo76",
  "preview": false,
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Microsoft/vscode-extension-samples"
  },
  "engines": {
    "vscode": "^1.74.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished"
  ],
  "main": "./out/extension.js",
  "icon": "media/favicon.png",
  "scripts": {
    "vscode:prepublish": "npm run build",
    "b": "npm run build",
    "build": "node ./scripts/build.mjs",
    "dev": "node ./scripts/build.mjs --watch",
    "typecheck": "tsc --noEmit",
    "publish": "vsce package --no-dependencies"
  },
  "contributes": {
    "commands": [
      {
        "command": "searchx.searchInFolder",
        "title": "SearchPro: Find Text in Folder"
      },
      {
        "command": "searchx.searchInFile",
        "title": "SearchPro: Find Text in File"
      },
      {
        "command": "searchx.clearSearchResults",
        "title": "Clear Search Results",
        "icon": {
          "light": "media/clear-light.svg",
          "dark": "media/clear-dark.svg"
        }
      },
      {
        "command": "searchx.refreshSearch",
        "title": "Refresh Search",
        "icon": {
          "light": "media/refresh-light.svg",
          "dark": "media/refresh-dark.svg"
        }
      },
      {
        "command": "searchx.collapseAll",
        "title": "Collapse All",
        "icon": {
          "light": "media/collapse-light.svg",
          "dark": "media/collapse-dark.svg"
        }
      },
      {
        "command": "searchx.expandAll",
        "title": "Expand All",
        "icon": {
          "light": "media/expand-light.svg",
          "dark": "media/expand-dark.svg"
        }
      }
    ],
    "menus": {
      "explorer/context": [
        {
          "command": "searchx.searchInFolder",
          "group": "4_search",
          "when": "explorerResourceIsFolder"
        },
        {
          "command": "searchx.searchInFile",
          "group": "4_search",
          "when": "!explorerResourceIsFolder"
        }
      ],
      "editor/context": [
        {
          "command": "searchx.searchInFile",
          "group": "navigation"
        }
      ],
      "view/title": [
        {
          "command": "searchx.collapseAll",
          "when": "view == searchx.search.input",
          "group": "navigation"
        },
        {
          "command": "searchx.expandAll",
          "when": "view == searchx.search.input",
          "group": "navigation"
        },
        {
          "command": "searchx.clearSearchResults",
          "when": "view == searchx.search.input",
          "group": "navigation"
        },
        {
          "command": "searchx.refreshSearch",
          "when": "view == searchx.search.input",
          "group": "navigation"
        }
      ]
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "searchx-sidebar-view",
          "title": "Search Pro",
          "icon": "media/fuzzy.svg"
        }
      ]
    },
    "views": {
      "searchx-sidebar-view": [
        {
          "type": "webview",
          "id": "searchx.search.input",
          "name": "Search Pro"
        }
      ]
    }
  },
  "devDependencies": {
    "@stylexjs/esbuild-plugin": "0.7.5",
    "@types/escape-html": "^1.0.4",
    "@types/node": "^20",
    "@types/react": "18.3.5",
    "@types/react-dom": "18.3.0",
    "@types/vscode": "^1.73.0",
    "@types/vscode-webview": "^1.57.0",
    "esbuild": "0.23.1",
    "tslib": "2.7.0",
    "tsx": "4.19.0",
    "typescript": "^5.6.2",
    "unport": "0.6.0",
    "vsce": "^2.15.0"
  },
  "dependencies": {
    "chevrotain": "^11.0.3",
    "escape-html": "^1.0.3",
    "react-use-vue-reactive": "latest",
    "vue": "^3.5.12",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-icons": "5.3.0",
    "react-simple-code-editor": "^0.14.1",
    "react-use": "17.5.1",
    "react-virtuoso": "4.10.1",
    "@stylexjs/stylex": "0.7.5",
    "@vscode/webview-ui-toolkit": "1.4.0"
  }
}
